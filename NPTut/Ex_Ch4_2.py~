print("The High/Low Game!!")
val = 23752
guess = 0
count = 0
while guess != val:
    guess = input("What is your guess: ")
    if guess == val and count == 0:
        print("Amazing!! You got it on the first try!!")
        print(" Go to Vegas immediately")
    elif guess == val:
        print("Thats right, you got it in ",count," tries")
    elif guess<val:
        print("Too low.")
    elif guess>val:
        print("Too high.")
        
        
